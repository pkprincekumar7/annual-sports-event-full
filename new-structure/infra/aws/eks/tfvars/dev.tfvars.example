# Example values for dev
aws_region      = "us-east-1"
aws_account_id  = "123456789012"
cluster_name    = "annual-sports-dev"

availability_zones = ["us-east-1a", "us-east-1b"]
public_subnets      = ["10.10.1.0/24", "10.10.2.0/24"]
private_subnets     = ["10.10.11.0/24", "10.10.12.0/24"]

domain     = "dev.your-domain.com"
api_domain = "api.dev.your-domain.com"

image_tag = "latest"

mongo_uris = {
  "identity-service"             = "mongodb+srv://user:pass@cluster/annual-sports-identity-dev"
  "enrollment-service"           = "mongodb+srv://user:pass@cluster/annual-sports-enrollment-dev"
  "department-service"           = "mongodb+srv://user:pass@cluster/annual-sports-department-dev"
  "sports-participation-service" = "mongodb+srv://user:pass@cluster/annual-sports-participation-dev"
  "event-configuration-service"  = "mongodb+srv://user:pass@cluster/annual-sports-event-config-dev"
  "scheduling-service"           = "mongodb+srv://user:pass@cluster/annual-sports-scheduling-dev"
  "scoring-service"              = "mongodb+srv://user:pass@cluster/annual-sports-scoring-dev"
  "reporting-service"            = "mongodb+srv://user:pass@cluster/annual-sports-reporting-dev"
}

database_names = {
  "identity-service"             = "annual-sports-identity-dev"
  "enrollment-service"           = "annual-sports-enrollment-dev"
  "department-service"           = "annual-sports-department-dev"
  "sports-participation-service" = "annual-sports-participation-dev"
  "event-configuration-service"  = "annual-sports-event-config-dev"
  "scheduling-service"           = "annual-sports-scheduling-dev"
  "scoring-service"              = "annual-sports-scoring-dev"
  "reporting-service"            = "annual-sports-reporting-dev"
}

jwt_secret = "replace-with-strong-secret"
email_provider = "gmail"
gmail_user = "your-email@gmail.com"
gmail_app_password = "your-app-password"
email_from = "no-reply@your-domain.com"
